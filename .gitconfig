[color]
  ui = auto
  diff = auto
  interactive = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[push]
  # instead of pushing all branches with git push, you only push the current one.
  default = current
[core]
  # global per user gitignore file
  excludesfile = "~/.gitignore"
[status]
  [submodule]
    summary = 0
[branch]
  autosetupmerge = true
[gitreview]
	username = tjarvstrand
